package bangun;

public class lingkaran {
	public static void main(String[] args){
        double d, kl;
        d = 10;
        System.out.println("Kalkulator Keliling Lingkaran");
        System.out.println("Diameter = " + d);
        kl = d * 3.14;
        System.out.print("Keliling Lingkaran = " + kl);
    }
}
